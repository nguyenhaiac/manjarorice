def_key "u"
    scroll_up

def_key "e"
    scroll_down

def_key "ctrl-u"
    page_up

def_key "ctrl-e"
    page_down

def_key "n"
    previous_column

def_key "i"
    next_column

def_key "."
    show_lyrics

def_key "y"
    next_found_item

def_key "l"
    previous_found_item

def_key "f"
    show_browser

def_key "f"
    change_browse_mode

def_key "P"
    show_playlist
