super + shift + l
	pkill -USR1 -x sxhkd; \
	notify-send "sxhkd restarted"

super + j
      openemacs

super + m
      rofi -show combi

super + shift + j
      emacsclient -nc

XF86AudioRaiseVolume
      pactl set-sink-volume @DEFAULT_SINK@ +2%

XF86AudioLowerVolume
      pactl set-sink-volume @DEFAULT_SINK@ -2%

super + o; {f, a, b}
      {firefox, anki, bookfinder}

Print
      maimpick

